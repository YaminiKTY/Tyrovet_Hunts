package com.tyss.talunthunt.dto;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;
import javax.persistence.Table;

import lombok.Data;

@Entity
@Table(name = "month_Bean")
@Data
public class MonthlyBean {
	@Id
	@GeneratedValue
	@Column
	private Integer monthId;
	@Column
	private int jan;
	@Column
	private int feb;
	@Column
	private int mar;
	@Column
	private int apr;
	@Column
	private int may;
	@Column
	private int jun;
	@Column
	private int jul;
	@Column
	private int aug;
	@Column
	private int sep;
	@Column
	private int oct;
	@Column
	private int nov;
	@Column
	private int dec;
}
